<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>

<VBox alignment="TOP_CENTER" prefHeight="800.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.MainMenuGameController">
   <children>
      <Label text="Geometric Warfare" textAlignment="CENTER">
         <font>
            <Font name="System Italic" size="48.0" />
         </font>
         <padding>
         <Insets top="200.0" />
         </padding>
      </Label>
      <VBox alignment="CENTER" spacing="40.0">
      <children><Button onAction="#newGame" prefWidth="200.0" text="New Game" textAlignment="CENTER" />
      <Button mnemonicParsing="false" onAction="#leaderboard" prefWidth="200.0" text="Leaderboard" />
      <Button mnemonicParsing="false" onAction="#exit" prefWidth="200.0" text="Exit" />
      </children>
      <padding>
      <Insets top="150.0" /></padding>
      </VBox>
   </children>
</VBox>
