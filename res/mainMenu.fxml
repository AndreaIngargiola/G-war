<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>

<VBox fx:id="primaryPane" alignment="TOP_CENTER" prefHeight="800.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.MainMenuGameController">
   <children>
      <Label fx:id="titleMainMenu" textAlignment="CENTER">
         <font>
            <Font name="System Italic" size="48.0" />
         </font>
         <padding>
            <Insets top="200.0" />
         </padding>
      </Label>
      <VBox fx:id="secondPane" alignment="BOTTOM_CENTER" prefHeight="300.0" prefWidth="500.0" spacing="10.0">
      <children><Button fx:id="newGameButton" mnemonicParsing="false" onAction="#newGame" prefHeight="20.0" prefWidth="200.0" textAlignment="CENTER" />
      <Button fx:id="leaderboardButton" mnemonicParsing="false" onAction="#leaderboard" prefWidth="200.0" />
      <Button fx:id="quitButton" mnemonicParsing="false" onAction="#exit" prefWidth="200.0" />
      </children>
         <padding>
            <Insets top="20.0" />
         </padding>
      </VBox>
   </children>
</VBox>
